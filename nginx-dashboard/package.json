{
  "name": "nginx-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:db": "cp app/dashboard/page-db.js app/dashboard/page.js && next dev --turbopack",
    "build:db": "cp app/dashboard/page-db.js app/dashboard/page.js && next build",
    "process:logs": "node ../log_processor.js",
    "generate:data": "node ../dashboard_data_generator.js",
    "monitor": "node ../log_monitor.js"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "date-fns": "^2.30.0",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "next": "15.3.2",
    "pg": "^8.11.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "node-cron": "^3.0.3",
    "chokidar": "^3.6.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4"
  }
}
